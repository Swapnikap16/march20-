//PROGRAM ON BIGGEST OF THREE NUMBERS USING RECURSSIVE FUNCTION//
#include<stdio.h>
#include<conio.h>
int biggest(int a,int b,int c);
main()
{
int a,b,c;
scanf("%d%d%d",&a,&b,&c);
int big=biggest(a,b,c);
printf("%d",big);
return 0;	
}
int biggest(int a,int b,int c)
{
	if(a>b&&a>c)
	return a;
	else if(b>a&&b>c)
	return b;
	else
	return c;	
}
